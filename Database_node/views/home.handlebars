{{#if results.length}}
	<h2>Sent Contents</h2>
	<table>
		<tr>
			<th>Name</th>
			<th>reps</th>
			<th>weight</th>
			<th>date</th>
			<th>lbs</th>
		</tr>
		{{#each results}}
			<tr>
				<td>{{this.name}}</td>
				<td>{{this.reps}}</td>
				<td>{{this.weight}}</td>
				<td>{{this.date}}</td>
				<td>{{this.lbs}}</td>
			</tr>
		{{/each}}
	</table>
{{/if}}

